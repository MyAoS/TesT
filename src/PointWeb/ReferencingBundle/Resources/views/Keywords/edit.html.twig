{% extends 'PointWebAdminBundle::base.html.twig' %}

{% block content_right %}
    <h1>Modifier la liste des mots-cl&eacute;s</h1>

    {{ form_start(edit_form, {'attr': {'class':'form-horizontal' }}) }}
    <fieldset>

        <div class="form-group">
            {{ form_label(edit_form.keywords) }}
            <div class="col-lg-10">
                {{ form_errors(edit_form.keywords) }}
                {{ form_widget(edit_form.keywords) }}
            </div>
        </div>

    </fieldset>
    {{ form_end(edit_form) }}

    <a href="{{ path('admin_keywords') }}" class="btn btn-primary"><i class="fa fa-arrow-circle-left"></i> Retour &agrave; la
        liste</a>
{% endblock %}
